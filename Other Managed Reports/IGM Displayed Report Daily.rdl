        <QueryParameters>
          <QueryParameter Name="@EndOfDay">
            <Value>=Parameters!EndOfDay.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* IGM Activation Daily Report - Displayed Measures	*/
/* Modified: 2014-09-29 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--  Variables only used for testing this script in SSMS
--DECLARE @EndOfDay DATETIME = '20140423';

--  Get processing dates
DECLARE @DateKey INT = CONVERT(INT, CONVERT(CHAR(8), @EndOfDay, 112));
DECLARE @CalDateDay DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

--Get game list for report
DECLARE @DimScratchersProductType TABLE 
(		ProductKey SMALLINT NOT NULL
,		ProductNumber VARCHAR(10) NOT NULL
,		PricePointAmount TINYINT NOT NULL
,		NewGameFlag CHAR(1) NOT NULL
,		IdealMixGameFlag CHAR(1) NOT NULL
,		EndedGameFlag CHAR(1) NOT NULL
,		PastEndedGame INT NOT NULL
)
INSERT INTO @DimScratchersProductType
SELECT	DISTINCT PT.ProductKey
,		ProductNumber
,		PricePointAmount
,		NewGameFlag = UPPER(PT.NewGameFlag)
,		PT.IdealMixGameFlag
,		EndedGameFlag = CASE WHEN DATEADD(DD,-223,P.ProductDisableDate) &lt; @CalDateDay THEN 'Y' ELSE 'N' END
,		PastEndedGame = CASE WHEN (DATEDIFF(DD, @EndOfDay, P.ProductDisableDate) &lt; = 181) THEN 1 ELSE 0 END -- Past (1) is a date beyond the Retailer End of Date, Current (0) is between 6 weeks Retailer End of Sale Date
FROM	BIDW.dbo.DimScratchersProductType PT
		INNER JOIN
		BIDW.dbo.DimProduct P
		ON	PT.ProductKey = P.ProductKey
		INNER JOIN
		BIDW.dbo.RptPenetrationSnapshotDaily RPS
		ON	PT.ProductKey=RPS.ProductKey
WHERE	ProductTypeCode = 1
AND		EffectiveStartDate &lt;= @EndOfDay
AND		COALESCE(EffectiveEndDate,'01/01/2038') &gt;= @EndOfDay
AND		ProductLaunchDate &lt;= CONVERT(DATE,@CalDateDay,101)
AND	(	PT.IdealMixGameFlag = 'Y' OR DATEADD(DD,-223,P.ProductDisableDate) &lt; @CalDateDay)
AND		DateKey=@DateKey;

--Get retailer base for report.
CREATE TABLE #RetailerBaseDailyReport
(		ProductKey SMALLINT NOT NULL
,		ProductNumber VARCHAR(10) NOT NULL
,		PricePointAmount TINYINT NOT NULL
,		NewGameFlag CHAR(1) NOT NULL
,		IdealMixGameFlag CHAR(1) NOT NULL
,		EndedGameFlag CHAR(1) NOT NULL
,		DateKey INT NOT NULL
,		Retailerkey INT NOT NULL
,		RetailerId VARCHAR(80) NOT NULL
,		RetailerName VARCHAR(255) NULL
,		[Address] VARCHAR(30) NULL
,		City VARCHAR(30) NULL
,		Zip VARCHAR(10) NULL
,		District VARCHAR(255) NULL
,		SalesRep INT NULL
,		LTSS INT NULL
,		KAS INT NULL
,		Chain VARCHAR(255) NULL
,		[Chain ID] VARCHAR(30) NULL
,		Classification VARCHAR(20) NULL
,		Facings INT NULL
,		SalesAmount DECIMAL(18,3) NULL
,		PrimaryPhoneNum VARCHAR(30) NULL
,		IPSCallDay VARCHAR(20) NULL
,		IPSCallWeek VARCHAR(20) NULL
,       IPSCallCycle VARCHAR(20) NULL
,		IPSCallWeekLabel VARCHAR(20) NULL
,		Active INT NOT NULL DEFAULT 0
,		Ended INT NOT NULL DEFAULT 0
,		PastEndedGame INT NOT NULL DEFAULT 0
,		CONSTRAINT PK_RetailerBaseIGMDisplayedReport
               PRIMARY KEY CLUSTERED (Retailerkey, ProductKey)
)
INSERT INTO #RetailerBaseDailyReport 
(		Retailerkey
,		DateKey
,		RetailerId
,		RetailerName
,		[Address]
,		City
,		Zip
,		District
,		SalesRep
,		LTSS
,		KAS
,		Chain
,		[Chain ID]
,		Classification
,		Facings
,		SalesAmount
,		PrimaryPhoneNum
,		IPSCallDay
,		IPSCallWeek
,       IPSCallCycle
,		IPSCallWeekLabel
,		ProductKey
,		ProductNumber
,		PricePointAmount
,		NewGameFlag
,		IdealMixGameFlag
,		EndedGameFlag
,		PastEndedGame
)
SELECT	RB.Retailerkey
,		DateKey
,		RB.RetailerId
,		RB.RetailerName
,		RB.[Address]
,		RB.City
,		RB.Zip
,		RB.District
,		RB.SalesRep
,		RB.LTSS
,		RB.KAS
,		RB.Chain
,		RB.[Chain ID]
,		RB.Classification
,		RB.Facings
,		RB.SalesAmount
,		R.PrimaryPhoneNum
,		R.IPSCallDay
,		R.IPSCallWeek
,       R.IPSCallCycle
,		IPSCallWeekLabel = CASE WHEN R.IPSCallWeek IS NULL AND R.IPSCallCycle &lt;&gt; 'Weekly' THEN 'A-Week'
							    WHEN R.IPSCallWeek = 'A-Week' and R.IPSCallCycle = 'Weekly' THEN 'A/B-Week'
							    WHEN R.IPSCallWeek = 'B-Week' and R.IPSCallCycle = 'Weekly' THEN 'A/B-Week'
							ELSE COALESCE(R.IPSCallWeek,'A/B-Week')
							END
,		ProductKey
,		ProductNumber
,		PricePointAmount
,		NewGameFlag = UPPER(NewGameFlag)
,		IdealMixGameFlag
,		EndedGameFlag
,		PastEndedGame
FROM	BIDW.dbo.LkpRetailerBaseDaily RB
		INNER JOIN BIDW.dbo.DimRetailer R
		ON	RB.Retailerkey = R.RetailerKey
		CROSS JOIN
		@DimScratchersProductType P
WHERE	DateKey = @DateKey
AND		COALESCE(R.ParentChainId, '') NOT IN ('1010116', '1024959')
AND		DateKey = @DateKey
AND		IPSOrderRestriction &lt;&gt; 'No Order';


UPDATE	#RetailerBaseDailyReport
SET		Active = CASE WHEN B.PacksDisplayedRetailerID IS NOT NULL THEN 1 ELSE 0 END
,		Ended = CASE WHEN A.EndedGameFlag = 'Y' AND (COALESCE(B.ActivatedRetailerID,B.ConfirmedRetailerID,B.IssuedRetailerID) IS NOT NULL) THEN 1 ELSE 0 END
FROM	#RetailerBaseDailyReport A
		INNER JOIN
		BIDW.dbo.RptPenetrationSnapshotDaily B
		ON	A.ProductKey = B.ProductKey
		AND	A.RetailerKey = B.RetailerKey
		AND A.DateKey = B.DateKey;

DELETE FROM #RetailerBaseDailyReport WHERE EndedGameFlag = 'Y' AND Ended = 0;

SELECT * FROM #RetailerBaseDailyReport ORDER BY RetailerId,NewGameFlag DESC,PricePointAmount DESC,ProductNumber DESC;

DROP TABLE #RetailerBaseDailyReport;</CommandText>
