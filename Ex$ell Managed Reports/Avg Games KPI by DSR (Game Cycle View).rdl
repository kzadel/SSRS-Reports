    <DataSet Name="CompletedWeekend">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>/* Get most recent full completed weekend date */
/* Modified: 2013-06-24 */

SELECT a.DateKey AS WeekEndDateKey
, CalDayDate
FROM BIDW.dbo.DimDate AS a 
INNER JOIN
(	SELECT DISTINCT TOP 1 WeekEndKey
	FROM BIDW.dbo.RptPenetrationSnapshot
	ORDER BY WeekEndKey DESC
) AS b 
ON a.DateKey = b.WeekEndKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
