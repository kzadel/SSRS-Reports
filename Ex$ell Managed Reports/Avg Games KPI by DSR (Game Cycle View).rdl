    <DataSet Name="IdealGameMixList">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@we">
            <Value>=Parameters!we.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* IdealGameMixList */
/* Modified 2015-01-30 */
/* Modified 2015-03-26 added new product groupings */
/* Modified 2015-05-21, 6-10,7-21 added new product groupings*/

--  Variables only used for testing this script in SSMS
--DECLARE @we INT = '20160725';
 
--  Move passed parameters into working fields
DECLARE @DateKey INT = @we;
--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

SELECT DISTINCT PricePoint = '$'+ CONVERT(VARCHAR(2), CONVERT(INT,PricePointAmount))
			,	P.PricePointAmount
			,	NewGameFlag = UPPER(NewGameFlag) 
			,ProductDesc = (CASE WHEN ProductGroup = '1201/1226' 
					THEN 'MEGA CROSSWORD/MEGA CROSSWORD'
					ELSE ProductDesc END)	
			,	GameNumber = (CASE WHEN ProductGroup IS NULL THEN CONVERT(varchar,P.ProductNumber) ELSE ProductGroup END)
			,	ProductLaunchDate = (CASE WHEN PG.ProductLaunchDate IS NULL THEN (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, P.ProductLaunchDate))), 101)) ELSE (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, PG.ProductLaunchDate))), 101)) END)
			,	EffectiveEndDate
FROM	BIDW.dbo.DimScratchersProductType PT
		INNER JOIN BIDW.dbo.DimProduct P
		ON PT.ProductKey = P.ProductKey
		LEFT OUTER JOIN 
		BIDW.dbo.LkpInstantProductGroup PG
		ON P.ProductNumber = PG.ProductNumber
WHERE ProductTypeCode = 1 
AND IdealMixGameFlag = 'Y'
AND WeeklyEffectiveStartDate &lt;= @CalDayDate
AND	P.ProductLaunchDate &lt; CONVERT(DATE,GETDATE(),101)
AND (COALESCE(WeeklyEffectiveEndDate,'01/01/2038') &gt;= @CalDayDate)
ORDER BY PricePointAmount, GameNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PricePoint">
          <DataField>PricePoint</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PricePointAmount">
          <DataField>PricePointAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NewGameFlag">
          <DataField>NewGameFlag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductDesc">
          <DataField>ProductDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GameNumber">
          <DataField>GameNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductLaunchDate">
          <DataField>ProductLaunchDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EffectiveEndDate">
          <DataField>EffectiveEndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
