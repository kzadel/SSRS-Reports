    <DataSet Name="WeekendDates">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>/* List of the weekend dates for the current game set  */
/* Modified 2013-09-30 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--Get most recent full completed weekend date
declare @we int = (SELECT a.DateKey AS WeekEndDateKey FROM BIDW.dbo.DimDate AS a 
													  INNER JOIN
													  (SELECT DISTINCT TOP 1 WeekEndKey
													   FROM BIDW.dbo.RptPenetrationSnapshot
													  ORDER BY WeekEndKey DESC
													  ) AS b 
													  ON a.DateKey = b.WeekEndKey )

--Move passed parameters into working fields
declare @DateKey int = @we;

--  Get processing dates
declare @CalDayDate datetime = (select top 1 CalDayDate from BIDW.dbo.DimDate where @DateKey = DateKey);
DECLARE @MaxWEDate DATE = (SELECT MAX(WeeklyEffectiveStartDate) FROM BIDW.dbo.DimScratchersProductType WHERE WeeklyEffectiveStartDate &lt;= @CalDayDate);

select DISTINCT WeekEndDateKey
From BIDW.dbo.DimDate
where CalDayDate BETWEEN @MaxWEDate and @CalDayDate
order by WeekEndDateKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="WeekEndDateKey">
          <DataField>WeekEndDateKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AvgGames">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@we">
            <Value>=Parameters!we.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@weDates">
            <Value>=Parameters!weDates.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Average Games KPI by DSR(Game Cycle View)   */
/* Modified 2013-10-22 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--  Variables only used for testing this script in SSMS
--declare @we int = 20130921;
--declare @weDates int = 20130921;

--  Move passed parameters into working fields
declare @DateKey int = @we;

--  Get processing dates
declare @CalDayDate datetime = (select top 1 CalDayDate from BIDW.dbo.DimDate where @DateKey = DateKey);

--Get game list for report
declare @DimScratchersProductType table 
(ProductKey smallint not null
,ProductNumber varchar(10) not null
,GameNumber varchar(30) not null
,PricePointAmount tinyint not null
,NewGameFlag char(1) not null
,IdealMixGameFlag char(1) not null)
insert into @DimScratchersProductType
select P.ProductKey
      ,P.ProductNumber
      ,GameNumber = (case when ProductGroup is null then convert(varchar,P.ProductNumber) else ProductGroup end)
      ,PricePointAmount
      ,NewGameFlag
      ,IdealMixGameFlag
from BIDW.dbo.DimScratchersProductType PT
inner join BIDW.dbo.DimProduct P
      on PT.ProductKey = P.ProductKey
left outer join BIDW.dbo.LkpInstantProductGroup PG
      on P.ProductNumber = PG.ProductNumber
where ProductTypeCode = 1
and   pt.WeeklyEffectiveStartDate &lt;= @CalDayDate
and	  pt.IdealMixGameFlag = 'Y'
and	  p.ProductLaunchDate &lt; CONVERT(DATE, GETDATE(), 101)
and   COALESCE(WeeklyEffectiveEndDate, '01/01/2038') &gt;= @CalDayDate
order by ProductNumber desc

--Get retailer base for report.
create table #RetailerBase
(Retailerkey int not null --PRIMARY KEY CLUSTERED
,WeekEndDateKey int not null
,CalDayStr varchar(30) not null
,DistrictCode varchar(30) null
,RetClassificationCode varchar(20) null
,District varchar(255) null
,SalesRepID int null
,RegionCode varchar(30) null
,RegionName varchar(255) null
,RetailerBase int not null default 1
,NewIGMGameCount int null
,NotNewIGMGameCount int null
,NewFiveDollarGameCount int null
,NotNewFiveDollarGameCount int null
,TenDollarGameCountCurr int null
)
insert into #RetailerBase (Retailerkey,WeekEndDateKey, CalDayStr, DistrictCode, SalesRepID, RetClassificationCode,District,RegionCode,RegionName)
select rb.Retailerkey
	  , rb.WeekEndDateKey
	  , d.CalDayStr 
      , r.DistrictCode 
      , r.SalesRepUserId
      , r.RetClassificationCode
      , r.DistrictName as District
      , r.RegionCode
      , r.RegionName 
from BIDW.dbo.LkpRetailerBase rb
inner join BIDW.dbo.DimRetailer r
      on rb.Retailerkey = r.RetailerKey
inner join BIDW.dbo.DimDate D
	  on rb.WeekEndDateKey = d.WeekEndDateKey
	  and CONVERT(DATE, CAST(rb.WeekEndDateKey AS VARCHAR(30))) = d.CalDayDate			--only return the selected weekend date	  
where rb.WeekEndDateKey IN (@weDates)
      and COALESCE(r.ParentChainId, '') NOT IN ('1010116', '1024959')
	  and r.RetClassificationCode in ('A','B','C')
	  and IPSOrderRestriction &lt;&gt; 'No Order';
	  
--Get detailed KPI data. 
update #RetailerBase
set	NewIGMGameCount = c.NewIGMGameCount
,	NotNewIGMGameCount = c.NotNewIGMGameCount
,	NewFiveDollarGameCount = c.NewFiveDollarGameCount
,	NotNewFiveDollarGameCount = c.NotNewFiveDollarGameCount
,	TenDollarGameCountCurr =  c.TenDollarGameCountCurr
from	#RetailerBase rb
		inner join
		(
		select	RetailerKey
		,		WeekEndKey
		,		NewIGMGameCount = sum(NewIGMGameCount)
		,		NotNewIGMGameCount = SUM(NotNewIGMGameCount)
		,		NewFiveDollarGameCount = sum(NewFiveDollarGameCount)
		,		NotNewFiveDollarGameCount = sum(NotNewFiveDollarGameCount)
		,		TenDollarGameCountCurr =  sum(TenDollarGameCountCurr)
		from	(
				select	RetailerKey
				,		WeekEndKey
				,		PricePointAmount
				,		NewGameFlag
				,		NewIGMGameCount = sum(case when NewGameFlag = 'Y' and ActivatedRetailerID is not null then 1 else 0 end)
				,		NotNewIGMGameCount = sum(case when NewGameFlag = 'N' and ActivatedRetailerID is not null then 1 else 0 end)
				,		NewFiveDollarGameCount = sum(case when NewGameFlag = 'Y' and PricePointAmount = 5 and ActivatedRetailerID is not null then 1 else 0 end)
				,		NotNewFiveDollarGameCount = sum(case when NewGameFlag = 'N' and PricePointAmount = 5 and ActivatedRetailerID is not null then 1 else 0 end)
				,		TenDollarGameCountCurr =  sum(case when PricePointAmount = 10 and ActivatedRetailerID is not null then 1 else 0 end)

				from	(  --This step is required due to product groupings.
						select	RetailerKey
						,		GameNumber
						,		PricePointAmount
						,		pt.NewGameFlag
						,		ActivatedRetailerID = max(ActivatedRetailerID)
						,		EverActivatedRetailerID = max(EverActivatedRetailerID)
						,		WeekEndKey
						from	BIDW.dbo.RptPenetrationSnapshot rps
						inner join @DimScratchersProductType pt
								on	rps.ProductKey = pt.ProductKey
						where rps.WeekEndKey IN (@weDates)
						group by  WeekEndKey, RetailerKey, GameNumber, PricePointAmount, pt.NewGameFlag
						) a
				group by WeekEndKey
				,		RetailerKey
				,		PricePointAmount
				,		NewGameFlag
				) b
		group by WeekEndKey, RetailerKey
		) c
		on	rb.Retailerkey=c.RetailerKey 
		and rb.WeekEndDateKey = c.WeekEndKey	

--Return data set for report.
select  WeekEndDateKey
	  , rb.CalDayStr
      ,rb.RegionCode 
      ,max(rb.RegionName) as RegionName
      ,rb.DistrictCode
      ,max(rb.District) as DistrictName
      ,rb.SalesRepID
      ,SUM(RetailerBase) as RetailerBase
      ,sum(NewIGMGameCount) as NewIGMGameCount
      ,SUM(NotNewIGMGameCount) as NotNewIGMGameCount
      ,sum(NewFiveDollarGameCount) as NewFiveDollarGameCount
      ,sum(NotNewFiveDollarGameCount) as NotNewFiveDollarGameCount
      ,sum(TenDollarGameCountCurr) as TenDollarGameCountCurr
from #RetailerBase rb
group by  WeekEndDateKey
	  , rb.CalDayStr
	  ,	rb.RegionCode
      , rb.DistrictCode
      , rb.SalesRepID
--order by rb.RegionCode
--      , rb.DistrictCode
order by WeekEndDateKey
drop table #RetailerBase</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
