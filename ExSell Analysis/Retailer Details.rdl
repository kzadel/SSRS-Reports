  <DataSets>
    <DataSet Name="RetailerDrilldownDetail">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@d">
            <Value>=Parameters!d.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RetailerId">
            <Value>=Parameters!RetailerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*	RetailerDrilldownDetail - Header */
/*  Modified: 2013-10-07 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--  Variables only used for testing this script in SSMS
--DECLARE @d INT = 20130925;
--DECLARE @RetailerId VARCHAR(23) = '1001002'

--  Move passed parameters into working fields
DECLARE @DateKey INT = @d;

--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

SELECT	DateKey
,		RB.RetailerId
,		R.RetailerName
,		R.ActivationDate
,		R.PrimaryAddress1
,		R.PrimaryCity
,		R.PrimaryPostalCode
,		R.PrimaryPhoneNum
,		R.ParentChainId
,		R.ParentChainName
,		R.NumOfFacings
,		R.DistrictName
,		R.SalesRepUserId
,		R.SalesRepName
,		R.AccountRepUserId
,		R.InstantRepUserId
,		R.RetClassificationCode
,		R.IPSCallDay
,		R.IPSCallWeek
,       R.IPSCallCycle
,		IPSCallWeekLabel = CASE WHEN R.IPSCallWeek IS NULL AND R.IPSCallCycle &lt;&gt; 'Weekly' THEN 'A-Week'
							    WHEN R.IPSCallCycle = 'Weekly' THEN 'A/B-Week'
							ELSE COALESCE(R.IPSCallWeek,'A/B-Week')
							END
,		R.RetailerStatusDesc
,		SettlementSales13WeeksAvg = SUM(RB.SettlementSales13WeeksAvg)
,		SettlementSales1Week = SUM(RB.SettlementSales1Week)
,		R.TradeChannelDesc

FROM	BIDW.dbo.RptPenetrationSnapshotDaily RB
		INNER JOIN BIDW.dbo.DimRetailer R
		ON	RB.Retailerkey = R.RetailerKey
WHERE	DateKey = @DateKey
--AND		COALESCE(R.ParentChainId, '') NOT IN ('1010116')
AND		RB.RetailerId IN (@RetailerId)
AND     RB.RetailerID NOT IN ('1')
GROUP BY
		DateKey
,		RB.RetailerId
,		R.RetailerName
,		R.ActivationDate
,		R.PrimaryAddress1
,		R.PrimaryCity
,		R.PrimaryPostalCode
,		R.PrimaryPhoneNum
,		R.ParentChainId
,		R.ParentChainName
,		R.NumOfFacings
,		R.DistrictName
,		R.SalesRepUserId
,		R.SalesRepName
,		R.AccountRepUserId
,		R.InstantRepUserId
,		R.RetClassificationCode
,		R.IPSCallDay
,		R.IPSCallWeek
,       R.IPSCallCycle
,		R.RetailerStatusDesc
,		R.TradeChannelDesc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
