            <Value>=Parameters!RetailerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* End of Game */
/* Modified: 2013-09-26 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

-- Variables only used for testing this script in SSMS
--DECLARE @d INT = '20130805';
--DECLARE @RetailerId INT = '1001002'


----  Move passed parameters into working fields
DECLARE @DateKey INT = @d;

--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

SELECT	R2.RetailerId
--,		R2.RetailerKey
--,		R2.RetailerName
--,		R2.PrimaryAddress1
--,		R2.PrimaryPostalCode
,		ProductNumber
,		SerialNumber = CONVERT(VARCHAR, SerialNumber)
,		CAST(PricePointAmount AS INT) AS PricePointAmount
,		ProductLaunchDate
,		ProductDesc
--,		IssuedDate = (CONVERT(VARCHAR, (CONVERT(Date,CONVERT(VARCHAR, IssuedDateKey))), 101))
--,		ConfirmedDate = (CONVERT(VARCHAR, (CONVERT(Date,CONVERT(VARCHAR, ConfirmedDateKey))), 101))
--,		ActivatedDate = (CONVERT(VARCHAR, (CONVERT(Date,CONVERT(VARCHAR, ActivatedDateKey))), 101))
--,		SettledDate = (CONVERT(VARCHAR, (CONVERT(Date,CONVERT(VARCHAR, SettledDateKey))), 101))
--,		R2.SalesRepUserId
--,		R2.SalesRepName
--,		P.ProductDisableDate
--,		PastEndedGame = CASE WHEN (DATEDIFF(DD,@CalDayDate,P.ProductDisableDate) &lt;= 181) THEN 1 ELSE 0 END --Past (1) is a date beyond the Retailer End of Sale Date, Current (0) is between 6 weeks Retail End of Sale Date
,		EndedGameDate = DATEADD(DD,-181,P.ProductDisableDate)			-- Retailer End of Sale Date
,		StatusDesc
,		LowTierValPct
--,		MethodSettled
FROM	BIDW.dbo.FactPackSnapshot PS
		INNER JOIN 
		BIDW.dbo.DimProduct P
		ON PS.ProductKey = P.ProductKey
		INNER JOIN 
		BIDW.dbo.DimRetailer R1
		ON PS.RetailerKey = R1.RetailerKey
		INNER JOIN 
		BIDW.dbo.DimRetailer R2
		ON R1.RetailerId = R2.RetailerId 
		AND R2.EffectiveStartDate &lt;= @CalDayDate 
		AND COALESCE(R2.EffectiveEndDate, '1/1/2038') &gt;= @CalDayDate
		AND	ProductLaunchDate &lt;= @CalDayDate
		INNER JOIN 
		BIDW.dbo.RptPenetrationSnapshotDaily RB
		ON R2.RetailerKey = RB.RetailerKey 
		AND RB.DateKey = @d
		AND RB.ProductKey = PS.ProductKey
WHERE(CASE WHEN DATEADD(DD,-223,P.ProductDisableDate) &lt;= @CalDayDate THEN 'Y' ELSE 'N' END = 'Y') -- Game has already ended
AND  COALESCE(MethodSettled, 0) = 0 -- Have not settled yet
AND StatusCode IN ('2', '3', '4') -- Issued, Activated, or Confirmed
AND R2.RetailerId IN (@RetailerId)
AND R2.RetailerId NOT IN ('1')
GROUP BY R2.RetailerId
,		R2.RetailerKey
,		R2.RetailerName
,		R2.PrimaryAddress1
,		R2.PrimaryPostalCode
,		ProductNumber
,		SerialNumber
,		PricePointAmount
,		ProductLaunchDate
,		ProductDesc
,		IssuedDateKey
,		ConfirmedDateKey
,		ActivatedDateKey
,		SettledDateKey
,		R2.SalesRepUserId
,		R2.SalesRepName
,		P.ProductDisableDate
,		StatusDesc
,		MethodSettled
,		LowTierValPct
ORDER BY EndedGameDate, RetailerId</CommandText>
