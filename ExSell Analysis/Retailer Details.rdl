    <DataSet Name="EquipmentType">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RetailerId">
            <Value>=Parameters!RetailerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--  Variables only used for testing this script in SSMS
--DECLARE @RetailerId VARCHAR(25) = '1014444';

SELECT RetailerId
,RE.EquipmentTypeKey
, EquipmentTypeDesc
, EquipmentTypeCode
,RetailerEquipmentDesc
FROM BIDW.Dbo.FactRetailerEquipment RE
INNER JOIN BIDW.dbo.DimEquipmentType ET
ON RE.EquipmentTypeKey = ET.EquipmentTypeKey
INNER JOIN BIDW.dbo.DimRetailer R
ON RE.RetailerKey = R.RetailerKey
WHERE StatusDesc = 'Installed'
AND(CAST(RetailerID AS VARCHAR(25)) IN (@RetailerId)) 
AND (
		EquipmentTypeCode = '62' -- In-Counter - 24 Bin
		OR EquipmentTypeCode = '20'-- In-Counter 12 bins
		OR EquipmentTypeCode = '27'-- In-Counter 16 bins - Complete
		OR EquipmentTypeCode = '26'-- In-Counter 16 bins - Conv
		OR EquipmentTypeCode = '61'-- In-Counter 20 Bin
		OR EquipmentTypeCode = '69'-- Instant Showcase
		OR EquipmentTypeCode = '12'-- Large Ticket Display Holder
		OR EquipmentTypeCode = '25'-- Vending Machine 16 bins 
		OR EquipmentTypeCode = '70'-- Vending Machine 20 bin-Connect
		OR EquipmentTypeCode = '59'-- Vending Machine 20 bins
		OR EquipmentTypeCode = '17'-- Vending Machine 4 bins
		OR EquipmentTypeCode = '18'-- Vending Machine 6 bins
		OR EquipmentTypeCode = '19'-- Vending Machine 8 bins																
	 )
AND RetailerId NOT IN ('1')
ORDER BY RetailerId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
