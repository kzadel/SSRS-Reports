    <DataSet Name="RetailerDrilldownTable">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@d">
            <Value>=Parameters!d.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RetailerId">
            <Value>=Parameters!RetailerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*	RetailerDrilldownDetail - Displayed Measures 	*/
/*  Modified: 2014-09-30	*/
/*  Removed hard-coded logic for Displayed Packs, getting Displayed Packs directly from RptPenetrationSnapshotDaily table*/

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--  Variables only used for testing this script in SSMS
--DECLARE @d INT = 20140804;
--DECLARE @RetailerId VARCHAR(23) = 107602

--  Move passed parameters into working fields
DECLARE @DateKey INT = @d;

--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

DECLARE	@PacksSoldLast7Days TABLE (
		RetailerKey INT NOT NULL
	,	ProductKey INT NOT NULL
	,	PacksSoldLast7Days DECIMAL(8,4) NULL
	,	LowTierWinnersCount4WeeksAvg DECIMAL(8,4) NULL
)
INSERT INTO @PacksSoldLast7Days
SELECT	D.RetailerKey
	,	D.ProductKey
	,	PacksSoldLast7Days = D.LowTierWinnersCount1Week / (P.NumberOfTicketsPerPack * P.LowTierOddsValue)
	,	LowTierWinnersCount4WeeksAvg
FROM BIDW.dbo.RptPenetrationSnapshotDaily D
INNER JOIN BIDW.dbo.DimProduct P
ON D.ProductKey = P.ProductKey 
WHERE
	D.DateKey = @DateKey 
	AND	P.ProductLaunchDate &lt;= @CalDayDate 
	AND ( (D.PacksIssued &gt; 0) OR (D.PacksConfirmed &gt; 0) OR (D.PacksActive &gt; 0) OR (D.IdealMixGameFlag = 'Y') OR (d.PacksDisplayed &gt; 0))


----Total Packs Displayed
--DECLARE @TotalDisplayedPacks TABLE
--(
--	RetailerId INT NOT NULL
--	, ProductKey INT NOT NULL
--	, ProductNumber INT NOT NULL
--	, PacksDisplayed INT NULL
--)
--INSERT INTO @TotalDisplayedPacks
--SELECT
--			R.RetailerId
--		,	P.ProductKey
--		,	P.ProductNumber
--		,	PacksDisplayed = SUM( CASE 
--				 WHEN (PS.[FullPackFlag] = '1'											-- Displayed Updated
--				-- Pack was activated within the last 180 days
--				AND CONVERT(date, CAST(PS.ActivatedDateKey AS varchar) , 112) BETWEEN  DATEADD(d, -180, CONVERT(date, CAST(@DateKey AS varchar) , 112)) AND CONVERT(date, CAST(GETDATE() AS varchar) , 112)		
--				AND PS.StatusCode IN (4,5) -- Pack has to be in Activated or Settled Status
--				AND (CASE WHEN PS.StatusCode = 5 AND PS.SettledDateKey IS NULL THEN 1 ELSE 0 END) = 0 -- Settled Status packs cannot have a NULL SettledDateKey
--				AND PS.[LowTierValPct] &lt; (							
--					CASE
--						WHEN P.[PricePointAmount] = 1 THEN 81
--						WHEN P.[PricePointAmount] = 2 THEN 78
--						WHEN P.[PricePointAmount] = 3 THEN 82
--						WHEN P.[PricePointAmount] = 5 THEN 85
--						WHEN P.[PricePointAmount] = 10 THEN 84
--						WHEN P.[PricePointAmount] = 20 THEN 85
--						ELSE 80
--					END)
--				 AND PS.[LowTierValPct] &gt; 0
--				 AND LowTierWinnersCount4WeeksAvg &gt; 0 
--			    AND   (       --Game End Dt less than or equal to today's date (Game Has Ended) AND is in Activated status			-
--						 ((DATEADD(DD,-181,P.ProductDisableDate) &lt;= @CalDayDate) AND StatusCode = 4) 
--						    OR -- Game has not reached retailer end of sales		
--							((@CalDayDate &lt; DATEADD(DD,-181,P.[ProductDisableDate])))
--					   ) 
--				 ) THEN 1	

--				WHEN (PS.[FullPackFlag] = '1'						-- Displayed Updated
--				-- Pack was activated within the last 180 days
--				AND CONVERT(date, CAST(PS.ActivatedDateKey AS varchar) , 112) BETWEEN  DATEADD(d, -180, CONVERT(date, CAST(@DateKey AS varchar) , 112)) AND CONVERT(date, CAST(GETDATE() AS varchar) , 112)	
--				AND PS.StatusCode IN (4,5) -- Pack has to be in Activated or Settled Status
--				AND (CASE WHEN PS.StatusCode = 5 AND PS.SettledDateKey IS NULL THEN 1 ELSE 0 END) = 0 -- Settled Status packs cannot have a NULL SettledDateKey
--				AND PS.[LowTierValPct] = 0
--				AND ((PS.[StatusCode] = 4 AND PS.[MethodActivated] != 7)	-- Activated,	but not TimeActivated
--						OR (PS.[StatusCode] = 5 AND PS.[MethodSettled] NOT IN (7,15,16,17) ) )	-- Settled, but not TimeSettled, Lost, Returned, Stolen
--				-- ActivatedDate &gt; 4 Weeks ago
--				AND (CONVERT(date, CAST(PS.ActivatedDateKey AS varchar) , 112) &gt; DATEADD(d, -28, CONVERT(date, CAST(@DateKey AS varchar) , 112)))  
--			    AND ((@CalDayDate &lt; DATEADD(DD,-181,P.[ProductDisableDate]))) --Game has not reached retailer end of sales
--				      ) THEN 1  -- Met all conditions, pack is displayed
--				  ELSE 0 END )
--		FROM
--			BIDW.dbo.FactPackSnapshot PS 
--				INNER JOIN 
--			BIDW.dbo.DimProduct P
--				ON PS.ProductKey = P.ProductKey 
--				INNER JOIN
--			BIDW.dbo.DimRetailer R
--				ON PS.RetailerKey = R.RetailerKey 		
--				INNER JOIN
--			BIDW.dbo.LkpRetailerBaseDaily RB
--				ON RB.RetailerId = R.RetailerId
--			LEFT OUTER JOIN				
--			@PacksSoldLast7Days SLD
--				ON SLD.RetailerKey = RB.RetailerKey 
--				AND SLD.ProductKey = PS.ProductKey
--		WHERE RB.DateKey = @DateKey
--			AND P.ProductLaunchDate &lt;= @CalDayDate		
--			AND R.RetailerId IN (@RetailerId)
--		GROUP BY R.RetailerId
--		,	P.ProductNumber	
--		,	P.ProductKey
--		,	P.ProductNumber
--		,	P.PricePointAmount
--		,	P.ProductLaunchDate
--		,	P.ProductDesc
--		,	P.ProductDisableDate

SELECT	c.RetailerID
,		a.ProductNumber
,		PricePointAmount
,		ProductLaunchDate
,		ProductDesc
,		IdealMixGameFlag = UPPER(IdealMixGameFlag)
,		EndedGameFlag
,		PacksIssued = CASE WHEN PacksIssued IS NOT NULL THEN PacksIssued ELSE 0 END
,		PacksConfirmed = CASE WHEN PacksConfirmed IS NOT NULL THEN PacksConfirmed ELSE 0 END
,		PacksActive = CASE WHEN PacksActive IS NOT NULL THEN PacksActive ELSE 0 END
--,		PacksDisplayed = CASE WHEN d.PacksDisplayed IS NOT NULL THEN d.PacksDisplayed ELSE 0 END		-- updated logic
,		PacksDisplayed = CASE WHEN a.PacksDisplayed IS NOT NULL THEN a.PacksDisplayed ELSE 0 END		-- this uses old logic
,		PacksSettled = CASE WHEN PacksSettled IS NOT NULL THEN PacksSettled ELSE 0 END
,		DateKeyLastActivated = CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar,DateKeyLastActivated), 101))) 
,		DateKeyLastConfirmed = CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar,DateKeyLastConfirmed), 101))) 
,		DateKeyLastSettled = CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar,DateKeyLastSettled), 101))) 
,		LastPackSettlementType		
,		LowTierWinnersCount1Week
,		TicketCount
,		LowTierOddsValue
,		ShowGameOnReport = CASE WHEN IdealMixGameFlag = 'Y'
						 --  OR COALESCE(d.PacksDisplayed,0) &gt; 0
						   OR COALESCE(a.PacksDisplayed,0) &gt; 0
						   OR COALESCE(PacksConfirmed,0) &gt; 0
						   OR COALESCE(PacksIssued,0) &gt; 0
						   OR COALESCE(PacksActive,0) &gt; 0
						   THEN 1 ELSE 0 END
FROM
		(	 -- get list of all current retailers
			SELECT retailerid
			, ProductKey
			from BIDW.dbo.RptPenetrationSnapshotDaily	
			WHERE	RetailerID IN (@RetailerId)
			AND DateKey IN (@DateKey)
		)c
		--LEFT OUTER JOIN
		--(	-- get list of all displayed packs 
		--	SELECT RetailerId
		--	, ProductKey
		--	, PacksDisplayed
		--	FROM @TotalDisplayedPacks
		--	WHERE	RetailerID IN (@RetailerId)
		--)d
		--ON c.RetailerID = d.RetailerId
		--and c.ProductKey = d.ProductKey
		LEFT OUTER JOIN	
		(
			SELECT	D.RetailerID
			,		D.RetailerKey
			,		D.ProductKey
			,		PacksIssued
			,		PacksConfirmed
			,		PacksActive
			,		PacksSettled
			,		PacksDisplayed
			,		DateKeyLastActivated
			,		DateKeyLastConfirmed
			,		DateKeyLastSettled
			,		ProductNumber
			,		ProductDesc
			,		CAST(PricePointAmount AS INT) AS PricePointAmount
			,		ProductLaunchDate
			,		IdealMixGameFlag
			,		P.ProductDisableDate
			,		EndedGameFlag = CASE WHEN DATEADD(DD,-223, P.ProductDisableDate) &lt;= @CalDayDate THEN 'Y' ELSE 'N' END -- Game has already ended
			,		LowTierWinnersCount1Week
			,		TicketCount=NumberOfTicketsPerPack
			,		LowTierOddsValue
			FROM	BIDW.dbo.DimRetailer R
					INNER JOIN
					BIDW.dbo.RptPenetrationSnapshotDaily D
					ON	D.RetailerKey = R.RetailerKey
					AND	D.DateKey=@DateKey
					INNER JOIN
					BIDW.dbo.DimProduct P
					ON D.ProductKey = P.ProductKey
					AND	ProductLaunchDate &lt;= @CalDayDate
			WHERE	R.RetailerID IN (@RetailerId)
			AND	(	PacksIssued&gt;0 OR PacksConfirmed&gt;0 OR PacksActive&gt;0   OR IdealMixGameFlag='Y' OR PacksDisplayed&gt;0    ) 
		) a	
		ON c.RetailerID = a.RetailerID
		and c.ProductKey = a.ProductKey
		LEFT OUTER JOIN
		(
		SELECT	D.RetailerID
		,		D.ProductKey
		,		LastPackSettlementType = CASE WHEN MAX(M.ReportDesc)!=MIN(M.ReportDesc) THEN 'See Details' ELSE MAX(M.ReportDesc) END
		FROM	BIDW.dbo.RptPenetrationSnapshotDaily D
				INNER JOIN 
				BIDW.dbo.DimRetailer R
				ON D.RetailerId = R.RetailerId
				INNER JOIN BIDW.dbo.FactPackSnapshot PS
				ON R.RetailerKey = PS.RetailerKey
				AND D.ProductKey = PS.ProductKey
				AND D.DateKeyLastSettled = PS.SettledDateKey
				LEFT OUTER JOIN BIDW.dbo.DimMethod M
				ON PS.MethodSettled = M.MethodCode
		WHERE	D.RetailerID IN (@RetailerId)
		AND	D.DateKey=@DateKey
		GROUP BY D.RetailerID,D.ProductKey
		) b
		ON	a.RetailerID=b.RetailerID
		AND	a.ProductKey=b.ProductKey
		
WHERE a.RetailerID NOT IN ('1')
ORDER BY ProductNumber DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
