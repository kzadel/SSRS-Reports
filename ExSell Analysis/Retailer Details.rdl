    <DataSet Name="TimeActivatedTimeSettledPacks">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@d">
            <Value>=Parameters!d.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RetailerId">
            <Value>=Parameters!RetailerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Time Activated or Settled (w/no val) Exception Report */
/* Modified: 2013-09-26 */

-- Variables only used for testing this script in SSMS
--DECLARE @d INT = '20130804'
--DECLARE @RetailerId INT  = '140808'

----  Move passed parameters into working fields
DECLARE @DateKey INT = @d;

--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);
DEClARE @StartDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,-89),@CalDayDate)); -- 28 days for the No Val Packs Cal

SELECT R2.RetailerId
	--,R2.RetailerName
	--,R2.PrimaryAddress1
	--,R2.PrimaryPostalCode
	--,R2.RetClassificationCode
	--,R2.SalesRepName
	--,R2.SalesRepUserId 
	,ProductNumber
	,PricePointAmount
	,ProductLaunchDate
	,ProductDesc
	,StatusCode
	,StatusDesc
	,SerialNumber
	--,LowTierValPct
    ,MethodActivated
    ,MethodSettled
	,StatusDate = (CASE WHEN StatusCode = 4 THEN (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, ActivatedDateKey))), 101)) ELSE (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, SettledDateKey))), 101)) END)
	,LowTierValPct
FROM BIDW.dbo.FactPackSnapshot PS
	 INNER JOIN BIDW.dbo.DimProduct P
	 ON PS.ProductKey = p.ProductKey
	 INNER JOIN BIDW.dbo.DimRetailer R1
	 ON PS.RetailerKey = R1.RetailerKey
	 INNER JOIN BIDW.dbo.DimRetailer R2
	 ON R1.RetailerId = R2.RetailerId AND R2.EffectiveStartDate &lt;= @CalDayDate 
	 AND COALESCE(R2.EffectiveEndDate, '1/1/2038') &gt;= @CalDayDate
	 INNER JOIN BIDW.dbo.LkpRetailerBaseDaily RB
	 ON R2.RetailerKey = RB.RetailerKey AND RB.DateKey = @d
WHERE ((StatusCode = 5 AND MethodSettled = 7 and LowTierValPct = 0) OR (StatusCode = 4 AND MethodActivated = 7))
AND	COALESCE(R2.ParentChainId, '') NOT IN ('1010116', '1024959')
AND R2.RetailerId IN (@RetailerId)
AND ((StatusCode = 4 AND ActivatedDateKey &gt;= @StartDateKey) OR (StatusCode = 5 AND SettledDateKey &gt;= @StartDateKey))
AND R2.RetailerId NOT IN ('1')
ORDER BY R2.SalesRepUserId, R2.RetailerId, SerialNumber, StatusCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
