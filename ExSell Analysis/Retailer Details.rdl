        <CommandText>/*	LastVisitDate */
/*  Modified: 2013-09-26 */

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;

--  Variables only used for testing this script in SSMS
--DECLARE @RetailerId VARCHAR(23) = '1001002'

SELECT DISTINCT 
	RetailerId
	,VisitationDate = (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, MAX(VS.DateKey)))), 101))
FROM BIDW.dbo.FactRetailerVisitSummary VS
	INNER JOIN BIDW.dbo.DimSalesRep VSR
	ON VS.VisitSalesRepUserKey = VSR.SalesRepKey
	INNER JOIN BIDW.dbo.DimRetailer R
	ON VS.RetailerKey = R.RetailerKey
	LEFT OUTER JOIN BIDW.dbo.DimSalesRepRoles VSRR
	ON VSR.SalesRepUserId = VSRR.UserID
WHERE (DistrictName IS NOT NULL) 
AND (DistrictCode &lt;&gt; '035') 
AND (DistrictName NOT IN ('Redding', 'Bakersfield', 'Ventura', ''))
AND RetailerId IN (@RetailerId)
AND RetailerId NOT IN ('1')
GROUP BY RetailerId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
