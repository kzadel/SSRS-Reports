 iif( (Fields!TotalActivated.Value / Fields!Facings.Value ) &gt;= .60 and (Fields!TotalActivated.Value / Fields!Facings.Value ) &lt; .80, "PaleGoldenRod",
 iif( (Fields!TotalActivated.Value / Fields!Facings.Value ) &lt; .60, "LightCoral",
 "White"))))</BackgroundColor>
