      ,[Classification]
      ,[Facings]
      ,[SalesAmount]
      ,[PrimaryPhoneNum]
      ,[IPSCallDay]
      ,[IPSCallWeek]
      ,[IPSCallCycle]
      ,[IPSCallWeekLabel]
      ,[Active]
      ,[TotalActivated]
      ,[Ended]
      ,[PastEndedGame]
      ,[TradeStyle]
      ,[HadActive]
      ,[NeverHadActive]
      ,[Confirmed]
      ,[Issued]
      ,[CreatedPacks]
      ,[ET]
      ,[E0]
      ,[E1]
      ,[E2]
      ,[E3]
      ,[E4]
