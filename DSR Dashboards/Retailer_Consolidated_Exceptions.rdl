    <DataSet Name="DSR">
      <Query>
        <DataSourceName>BIBAReports</DataSourceName>
        <CommandText>/* List of DSRs */
/* Modified: 2013-09-16 */

SELECT DISTINCT SalesRep,DSRInfo = CONVERT(VARCHAR,SalesRep) + ' - ' + SalesRepName, SalesRepName
FROM BIBAReports.dbo.ConsolidatedExceptionsByRetailer
WHERE SalesRep &lt;&gt; '0'
ORDER BY SalesRepName, SalesRep  </CommandText>
