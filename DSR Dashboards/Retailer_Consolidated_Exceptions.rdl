      ,[Wk1SalesAmt]
      ,[Wk2SalesAmt]
      ,[Wk3SalesAmt]
      ,[Wk4SalesAmt]
      ,[TrendW1toW2]
      ,[TrendW2toW3]
      ,[TrendW3toW4]
      ,[LastVisitDate]
      ,[DateRange]
      ,[NoValPacksCt]
      ,[TotalEndedGames]
  FROM [BIBAReports].[dbo].[ConsolidatedExceptionsByRetailer]
  WHERE Classification IN (@Classification)
  AND SalesRep IN (@DSR)
  AND Zip IN (@Zip)
  AND COALESCE([Chain ID],'') NOT IN ('1010116')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
