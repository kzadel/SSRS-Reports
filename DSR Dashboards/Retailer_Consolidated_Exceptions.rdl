        <CommandText>/* Retailer Consolidated Exceptions - DisplayedMeasures */
/* Modified: 2013-10-14 */

--  Variables only used for testing this script in SSMS
--DECLARE @Classification VARCHAR(25) = 'C';
--DECLARE @DSR VARCHAR(250) = '21026';
--DECLARE @Zip VARCHAR(250) = '95901';

SELECT [ProductKey]
      ,[ProductNumber]
      ,[PricePointAmount]
      ,[NewGameFlag]
      ,[IdealMixGameFlag]
      ,[EndedGameFlag]
      ,[DateKey]
      ,[Retailerkey]
      ,[RetailerId]
      ,[RetailerName]
      ,[Address]
      ,[City]
      ,[Zip]
      ,[District]
      ,[SalesRep]
      ,[SalesRepName]
      ,[LTSS]
      ,[KAS]
      ,[Chain]
