    <DataSet Name="EndOfDay">
      <Query>
        <DataSourceName>BIBAReports</DataSourceName>
        <CommandText>/* Get the date in the RetailerConsolidatedExceptions table  */
/* Modified: 2013-09-16 */

SELECT TOP 1 DateKey
, CalDayStr = SUBSTRING( CONVERT(VARCHAR(10), DateKey),5, 2) + '/' + RIGHT(DateKey, 2) + '/' + LEFT(DateKey, 4) 
FROM BIBAReports.dbo.ConsolidatedExceptionsByRetailer</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Datekey">
          <DataField>DateKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CalDayStr">
          <DataField>CalDayStr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Classification">
      <Query>
        <DataSourceName>BIBAReports</DataSourceName>
        <CommandText>/* Classification */
/* Modified: 2013-09-16 */

SELECT DISTINCT Classification
FROM BIBAReports.dbo.ConsolidatedExceptionsByRetailer
ORDER BY Classification</CommandText>
      </Query>
      <Fields>
        <Field Name="Classification">
          <DataField>Classification</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ClassificationABC">
      <Query>
        <DataSourceName>BIBAReports</DataSourceName>
        <CommandText>/* ClassificationDefault */
/* Modified: 2013-09-16 */

SELECT DISTINCT Classification
FROM BIBAReports.dbo.ConsolidatedExceptionsByRetailer
WHERE Classification IN ('A', 'B', 'C')
ORDER BY Classification</CommandText>
      </Query>
      <Fields>
        <Field Name="Classification">
          <DataField>Classification</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="District">
      <Query>
        <DataSourceName>BIBAReports</DataSourceName>
        <CommandText>/* Get List of Districts */
/* Modified: 2013-09-16 */

SELECT DISTINCT District
FROM BIBAReports.dbo.ConsolidatedExceptionsByRetailer
WHERE SalesRep &lt;&gt; 0
ORDER BY District</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
