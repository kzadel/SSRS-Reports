  <DataSets>
    <DataSet Name="District">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT 
                         RegionName, DistrictCode, RTRIM(DistrictName) AS DistrictName, CASE WHEN RTRIM(DistrictName) 
                         = 'Santa Fe' THEN 'Santa Fe Springs' ELSE RTRIM(DistrictName) END AS DistrictNameLabel
FROM            DimRetailer
WHERE        (DistrictName IS NOT NULL) AND (DistrictCode &lt;&gt; '035') AND (DistrictName NOT IN ('Redding', 'Bakersfield', 'Ventura', ''))
ORDER BY RegionName, DistrictCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RegionName">
          <DataField>RegionName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistrictCode">
          <DataField>DistrictCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistrictName">
          <DataField>DistrictName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistrictNameLabel">
          <DataField>DistrictNameLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSR">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@District">
            <Value>=Parameters!District.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Variables used only in SSMS
--DECLARE @District VARCHAR(250) = '021';

SELECT DISTINCT SalesRepUserId
, SalesRepName
, CAST(SalesRepUserId AS varchar) + ' - ' + UPPER(SalesRepName) AS DSRInfo
FROM            DimRetailer
WHERE        (SalesRepUserId IS NOT NULL) 
AND (CurrentFlag = 'Y') 
AND DistrictName IN (@District)
ORDER BY SalesRepUserId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SalesRepUserId">
          <DataField>SalesRepUserId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesRepName">
          <DataField>SalesRepName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DSRInfo">
          <DataField>DSRInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RetNotVisitedLastXDays">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@District">
            <Value>=Parameters!District.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@we">
            <Value>=Parameters!we.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeChain">
            <Value>=Parameters!ExcludeChain.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@days">
            <Value>=Parameters!days.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DSR">
            <Value>=Parameters!DSR.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RClass">
            <Value>=Parameters!RClass.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*	Retailers Not Visited - RetNotVisitedLastXDays Dataset	*/
/* Modified: 2013-06-28 */

-- Variables used only in SSMS
 --DECLARE @District VARCHAR(250) = 'Sacramento';
 --DECLARE @DSR VARCHAR(250) = '21026';
 --DECLARE @RClass VARCHAR(25) = 'C';
 --DECLARE @we INT = '20121103'
 --DECLARE @days INT = '-13'  --14 days
 --DECLARE @ExcludeChain CHAR(1) = '1' -- 1 means exclude 1010116 and 1024959, 2 means include 1010116 and 1024959
 
----  Move passed parameters into working fields
DECLARE @DateKey INT = @we;

--  Get processing dates
DECLARE @CalDayDate DATETIME = (SELECT TOP 1 CalDayDate FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);
-- check to see if a daily date or a week end date was selected
DECLARE @CurrentFlag CHAR (1) = (SELECT CASE WHEN WeekEndDateKey = DateKey THEN 'N' ELSE 'Y' END FROM BIDW.dbo.DimDate WHERE @DateKey = DateKey);

DEClARE @StartDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,@days),@CalDayDate));
DECLARE @ADateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT(INT,@days),@CalDayDate));
DECLARE @BDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd,CONVERT( INT,@days),@CalDayDate));
DECLARE @CDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,@days),@CalDayDate));
DECLARE @DDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,@days),@CalDayDate));
DECLARE @EDateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,@days),@CalDayDate));
DECLARE @NADateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, CONVERT( INT,@days),@CalDayDate));
DECLARE @180DateKey INT = (SELECT TOP 1 DateKey FROM BIDW.dbo.DimDate WHERE CalDayDate = DATEADD(dd, -179,@CalDayDate));

DECLARE @ExcludeOneChain CHAR(1) = (SELECT CASE WHEN '1' IN (@ExcludeChain) THEN 'Y' ELSE 'N' END);
DECLARE @IncludeAllChains CHAR(1) = (SELECT CASE WHEN  '2' IN (@ExcludeChain) THEN 'Y' ELSE 'N' END);

SELECT	@we AS DateSelected
,		R.RetailerID
,		R.ActivationDate
,		R.RetailerName
,		SalesRepUserId
,		R.SalesRepName
,		RetClassificationCode
,		PrimaryAddress1
,		PrimaryCity
,		PrimaryPostalCode
,		LastVisitDate 
,		VisitsLast180Days = COALESCE(VisitsLast180Days,0)
FROM	BIDW.dbo.DimRetailer R				-- get all retailers
		INNER JOIN
		BIDW.dbo.LkpRetailerBase RB			--get all active retailers 
		ON	R.RetailerKey = RB.RetailerKey
		AND RB.WeekEndDateKey = @DateKey
 		AND 'Y' = case when @IncludeAllChains='Y' then 'Y'
                           when @ExcludeOneChain='Y' 
                                    AND		COALESCE(ParentChainId, '') NOT IN ('1010116') THEN 'Y'
                           else 'N' end
		AND	DistrictName IN (@District)
		AND	SalesRepUserId IN (@DSR)
		AND	RetClassificationCode IN (@RClass)
		AND	1 = CASE WHEN @CurrentFlag = 'N' THEN 1 ELSE 0 END  --  Use weekly tables	
		LEFT OUTER JOIN
		(
		SELECT	RetailerID
		,		LastVisitDate = (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, MAX(DateKey)))), 101))
		,		LastVisitDateKey = MAX(DateKey)
		,		VisitsLast180Days = COUNT(DISTINCT CASE WHEN DateKey &gt;= @180DateKey THEN DateKey ELSE NULL END)
		FROM	BIDW.dbo.FactRetailerVisitSummary RV
				INNER JOIN
				BIDW.dbo.DimRetailer R
				ON	RV.RetailerKey=R.RetailerKey
		WHERE	RV.DateKey &lt;= @we
		GROUP BY RetailerID
		) RV
		ON R.RetailerID = RV.RetailerId
WHERE	COALESCE(RV.LastVisitDateKey, 0) &lt; (CASE WHEN RetClassificationCode IN ('A') THEN @ADateKey
											 WHEN RetClassificationCode IN ('B') THEN @BDateKey  
											 WHEN RetClassificationCode IN ('C') THEN @CDateKey
											 WHEN RetClassificationCode IN ('D') THEN @DDateKey
											 WHEN RetClassificationCode IN ('E') THEN @EDateKey
											 WHEN RetClassificationCode IN ('N/A') THEN @NADateKey
											 ELSE @StartDateKey
										END)
UNION ALL
SELECT	@we AS DateSelected
,		R.RetailerID
,		R.ActivationDate
,		R.RetailerName
,		SalesRepUserId
,		R.SalesRepName
,		RetClassificationCode
,		PrimaryAddress1
,		PrimaryCity
,		PrimaryPostalCode
,		LastVisitDate 
,		VisitsLast180Days = COALESCE(VisitsLast180Days,0)
FROM	BIDW.dbo.DimRetailer R						-- get all retailers
		INNER JOIN
		BIDW.dbo.LkpRetailerBaseDaily RB			--get all active retailers 
		ON	R.RetailerKey = RB.RetailerKey
		AND RB.DateKey= @DateKey
 		AND 'Y' = case when @IncludeAllChains='Y' then 'Y'
                           when @ExcludeOneChain='Y' 
                                    AND		COALESCE(ParentChainId, '') NOT IN ('1010116') THEN 'Y'
                           else 'N' end
		AND	DistrictName IN (@District)
		AND	SalesRepUserId IN (@DSR)
		AND	RetClassificationCode IN (@RClass)
		AND	1 = CASE WHEN @CurrentFlag = 'Y' THEN 1 ELSE 0 END  --  Use daily tables	
		LEFT OUTER JOIN
		(
		SELECT	RetailerID
		,		LastVisitDate = (CONVERT(varchar, (CONVERT(Date,CONVERT(Varchar, MAX(DateKey)))), 101))
		,		LastVisitDateKey = MAX(DateKey)
		,		VisitsLast180Days = COUNT(DISTINCT CASE WHEN DateKey &gt;= @180DateKey THEN DateKey ELSE NULL END)
		FROM	BIDW.dbo.FactRetailerVisitSummary RV
				INNER JOIN
				BIDW.dbo.DimRetailer R
				ON	RV.RetailerKey=R.RetailerKey
		WHERE	RV.DateKey &lt;= @we
		GROUP BY RetailerID
		) RV
		ON R.RetailerId = RV.RetailerId
WHERE	COALESCE(RV.LastVisitDateKey, 0) &lt; (CASE WHEN RetClassificationCode IN ('A') THEN @ADateKey
											 WHEN RetClassificationCode IN ('B') THEN @BDateKey  
											 WHEN RetClassificationCode IN ('C') THEN @CDateKey
											 WHEN RetClassificationCode IN ('D') THEN @DDateKey
											 WHEN RetClassificationCode IN ('E') THEN @EDateKey
											 WHEN RetClassificationCode IN ('N/A') THEN @NADateKey
											 ELSE @StartDateKey
										END)
ORDER BY LastVisitDate ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RetailerID">
          <DataField>RetailerID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateSelected">
          <DataField>DateSelected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RetailerName">
          <DataField>RetailerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ActivationDate">
          <DataField>ActivationDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SalesRepUserId">
          <DataField>SalesRepUserId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesRepName">
          <DataField>SalesRepName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RetClassificationCode">
          <DataField>RetClassificationCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrimaryAddress1">
          <DataField>PrimaryAddress1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrimaryCity">
          <DataField>PrimaryCity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrimaryPostalCode">
          <DataField>PrimaryPostalCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastVisitDate">
          <DataField>LastVisitDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VisitsLast180Days">
          <DataField>VisitsLast180Days</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RClass">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT RTRIM(RetClassificationCode) AS RetClassificationCode
FROM            DimRetailer
WHERE        (RetailerStatusCode NOT IN (4, 7))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RetClassificationCode">
          <DataField>RetClassificationCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="we">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>DECLARE @Date DATETIME = GETDATE();
DECLARE @Year VARCHAR(30) = CAST(YEAR(DATEADD(DD,-6,@Date)) AS VARCHAR(30));
DECLARE @Month VARCHAR(30) = CAST(MONTH(DATEADD(DD,-6,@Date)) AS VARCHAR(30));

SELECT        a.DateKey AS WeekEndDateKey, a.CalDayStr AS CalDayDate
FROM            DimDate AS a INNER JOIN
                             (SELECT DISTINCT DateKey
                               FROM            LkpRetailerBaseDaily) AS b ON a.DateKey = b.DateKey
UNION
SELECT      DISTINCT 
            WeekEndDateKey
,			CONVERT(varchar(2), MONTH(CONVERT(varchar, CONVERT(Date, CONVERT(Varchar, WeekEndDateKey)), 101))) + '/' + CONVERT(varchar(2), 
                         DAY(CONVERT(varchar, CONVERT(Date, CONVERT(Varchar, WeekEndDateKey)), 101))) + '/' + CONVERT(varchar(4), YEAR(CONVERT(varchar, CONVERT(Date, 
                         CONVERT(Varchar, WeekEndDateKey)), 101))) AS WeekendString2
FROM  DimDate
WHERE FscYearStr = CASE WHEN @Month BETWEEN 1 AND 6 THEN @Year ELSE @Year + 1 END
AND         ReportWeekEndDate &lt;= @Date
ORDER BY WeekEndDateKey DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="WeekEndDateKey">
          <DataField>WeekEndDateKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CalDayDate">
          <DataField>CalDayDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="days">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT (DayNum * -1) AS DayNumKey
, CAST((DayNum * - 1) * -1 + 1 AS VARCHAR(8)) + ' days' AS DaysInfo
FROM    DimReportPeriod
WHERE   (DayNum = 6) -- 7 days
OR	(DayNum = 13)  -- 14 days
OR	(DayNum = 27) -- 28 days
OR	(DayNum =  179) -- 180 days
ORDER BY DayNumKey DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DayNumKey">
          <DataField>DayNumKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DaysInfo">
          <DataField>DaysInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="daysDefault">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT (DayNum * -1) AS DayNumKey
, CAST(DayNum + 1 AS VARCHAR(8)) + ' days' AS DaysInfo
FROM    DimReportPeriod
WHERE   (DayNum =  13) -- 14 days
ORDER BY DayNumKey DESC, DaysInfo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DayNumKey">
          <DataField>DayNumKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DaysInfo">
          <DataField>DaysInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ExcludeChain">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT 
      ExcludeChainNumKey = [DayNum]
     ,ExcludeChainInfo = (CASE WHEN [DayNum] = 1 THEN 'Y' 
					   WHEN [DayNum] = 2 THEN 'N'
				  END
				)
  FROM [BIDW].[dbo].[DimReportPeriod]
  WHERE DayNum = 1 or DayNum = 2
  ORDER by DayNum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ExcludeChainNumKey">
          <DataField>ExcludeChainNumKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ExcludeChainInfo">
          <DataField>ExcludeChainInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
