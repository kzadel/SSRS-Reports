    <DataSet Name="MajorChainName">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KAS">
            <Value>=Parameters!KAS.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Modified: 2013-08-08 */

SELECT DISTINCT MajorChainName
, MajorChainNameValue = CASE WHEN MajorChainName = 'RALEY''S/BEL AIR' THEN 'Raleys' ELSE MajorChainName END
FROM            BIDW.dbo.DimRetailer
WHERE        (MajorChainName IS NOT NULL)
AND (CurrentFlag = 'Y') 
AND (RetailerStatusCode NOT IN (4, 7)) 
AND AccountRepUserId IN (@KAS)
ORDER BY MajorChainName</CommandText>
      </Query>
      <Fields>
        <Field Name="MajorChainName">
          <DataField>MajorChainName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MajorChainNameValue">
          <DataField>MajorChainNameValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="KAS">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <CommandText>SELECT DISTINCT AccountRepUserId 
				,AccountRepName
				,AccountNameLength = (CASE WHEN UPPER(AccountRepName) = 'VACANT VACANT' THEN 100 ELSE LEN(AccountRepName) END)
				,CAST(AccountRepUserId AS varchar) + ' - ' + UPPER(AccountRepName) AS KASInfo
				
FROM            DimRetailer
WHERE			(AccountRepUserId IS NOT NULL) 
AND				(CurrentFlag = 'Y') 
AND				(RetailerStatusCode NOT IN (4, 7)) 
AND				(AccountRepUserId &lt;&gt; 35096)
ORDER BY AccountNameLength</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AccountRepUserId">
          <DataField>AccountRepUserId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccountRepName">
          <DataField>AccountRepName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="KASInfo">
          <DataField>KASInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccountNameLength">
          <DataField>AccountNameLength</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Chain">
      <Query>
        <DataSourceName>BIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MajorChainName">
            <Value>=Parameters!MajorChainName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@KAS">
            <Value>=Parameters!KAS.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Modified: 2013-08-08 */

/* Variables using only in SSMS */
--DECLARE @MajorChainName varchar(255) = 'Raleys'
--DECLARE @KAS int = '20173'


SELECT DISTINCT MajorChainName
,MajorChainNameValue = CASE WHEN MajorChainName = 'RALEY''S/BEL AIR' THEN 'Raleys' ELSE MajorChainName END
, ParentChainName
FROM BIDW.dbo.DimRetailer A
INNER JOIN
(
	SELECT DISTINCT Chain 
	FROM	BIDW.dbo.LkpRetailerBaseDaily B
) B
ON A.ParentChainName =  B.Chain
WHERE CASE WHEN MajorChainName = 'RALEY''S/BEL AIR' THEN 'Raleys' ELSE MajorChainName END IN (@MajorChainName)
AND AccountRepUserId IN (@KAS)
AND CurrentFlag = 'Y'
ORDER BY MajorChainName ASC, ParentChainName ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="MajorChainName">
          <DataField>MajorChainName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ParentChainName">
          <DataField>ParentChainName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MajorChainNameValue">
          <DataField>MajorChainNameValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
