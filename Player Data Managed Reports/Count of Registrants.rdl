FROM
(
		SELECT DateKey = convert(int, convert(varchar(10), PR.EffectiveStartDate, 112))
		, PlayerCreateDate = convert(int, convert(varchar(10), PR.PlayerCreateDateTime, 112))
		, PlayerRegistrationKey
		, PlayerMarketingKey
		, PR.PlayerID
		, PlayerActivationStatus
		, PM.PlayerPromotionalEmails
		, PlayerSecondChanceServiceID
		, PlayerPortalStatusID
		, PR.PlayerJackpotCaptain
		, PR.EffectiveStartDate
		, PR.EffectiveEndDate
		FROM BIDW.dbo.DimPlayerRegistration PR
		INNER JOIN BIDW.dbo.DimPlayerMarketing PM
		ON PR.PlayerID = PM.PlayerID
		WHERE convert(int, convert(varchar(10), PR.EffectiveStartDate, 112)) &gt;= COALESCE(convert(int, convert(varchar(10), PM.EffectiveStartDate, 112)), convert(int, convert(varchar(10), GETDATE(), 112))) -- PR Effective Start has be greater than or equals to PM Effective Start
		AND COALESCE(convert(int, convert(varchar(10), PR.EffectiveEndDate, 112)), convert(int, convert(varchar(10), GETDATE(), 112))) &lt;= COALESCE(convert(int, convert(varchar(10), PM.EffectiveEndDate, 112)), convert(int, convert(varchar(10), GETDATE(), 112))) -- PR Effective End has to be greater than or equals to PM Effective End	
		and PR.PlayerID NOT IN  (-1, -2, -3)
		--AND @StartDate BETWEEN PR.EffectiveStartDate AND ISNULL(PR.EffectiveEndDate,'2021-12-31')			-- StartDate has to be between the Effective Start and End Date
		--AND @StartDate BETWEEN PR.EffectiveStartDate AND COALESCE(PR.EffectiveEndDate, GETDATE()) --COALESCE(convert(int, convert(varchar(10), PM.EffectiveEndDate, 112)), convert(int, convert(varchar(10), GETDATE(), 112)))
		group by convert(int, convert(varchar(10), PR.PlayerCreateDateTime, 112))
		, convert(int, convert(varchar(10), PR.EffectiveStartDate, 112))
		, PlayerRegistrationKey
		, PlayerMarketingKey
		, PR.PlayerID
		, PlayerActivationStatus
		, PM.PlayerPromotionalEmails
		, PlayerSecondChanceServiceID
