INSERT INTO @TotalRegistrants
SELECT  DateKey
, TotalNewRegistrants = SUM(CASE WHEN PlayerCreateDate = DateKey THEN 1 ELSE 0 END)
, TotalActiveNewRegistrants = SUM(CASE WHEN PlayerCreateDate = DateKey AND PlayerActivationStatus =1 THEN 1 ELSE 0 END)
, ActivePP2ndChance = SUM(CASE WHEN PlayerActivationStatus = 1 
								AND PlayerSecondChanceServiceID = 2 -- Active
								AND PlayerPortalStatusID = 2 -- Active
								AND @StartDate BETWEEN EffectiveStartDate AND COALESCE(EffectiveEndDate, GETDATE())
							THEN 1 ELSE 0 END)
, ActiveCommOptIn = SUM(CASE WHEN PlayerActivationStatus = 1 
								AND PlayerSecondChanceServiceID = 2 -- Active
								AND PlayerPortalStatusID = 2 -- Active
								AND PlayerPromotionalEmails = 1
								AND @StartDate BETWEEN EffectiveStartDate AND COALESCE(EffectiveEndDate, GETDATE())
							THEN 1 ELSE 0 END)
, ActiveJackpotCaptain = SUM(CASE WHEN PlayerActivationStatus = 1 
								AND PlayerSecondChanceServiceID = 2 -- Active
								AND PlayerPortalStatusID = 2 -- Active
