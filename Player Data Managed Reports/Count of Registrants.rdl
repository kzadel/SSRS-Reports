/* Total Logins */  -- Login Data Starts on 10/14/13
DECLARE @TotalLogins TABLE
(
	DateKey INT NOT NULL
	, TotalActiveLogins INT NULL
	, TotalActiveNewLogins INT NULL
)
INSERT INTO @TotalLogins
SELECT D.DateKey
, TotalActiveLogins = SUM(CASE WHEN PlayerActivationStatus = 1 THEN VisitCount ELSE 0 END)
, TotalActiveNewLogins = SUM(CASE WHEN (convert(int, convert(varchar(10), PM.PlayerCreateDateTime, 112)) = D.DateKey) AND PlayerActivationStatus = 1 THEN VisitCount ELSE 0 END)
FROM BIDW.dbo.DimDate D
LEFT OUTER JOIN BIDW.dbo.FactLogins FL
ON D.DateKey = FL.DateKey
LEFT OUTER JOIN BIDW.dbo.DimPlayerMarketing PM
ON FL.PlayerMarketingKey = PM.PlayerMarketingKey
where D.DateKey BETWEEN @StartDateKey29 and @StartDateKey 
group by D.DateKey

select TR.Datekey
, CalDayStr
, TotalNewRegistrants = SUM(TotalNewRegistrants)
, TotalNewActiveRegistrants = SUM(TotalNewActiveRegistrants)
, ActivePP2ndChance = (SELECT SUM(ActivePP2ndChance) FROM @TotalRegistrants t WHERE t.DateKey &lt;= TR.DateKey)
, ActiveCommOptIn = (SELECT SUM(ActiveCommOptIn) FROM @TotalRegistrants t WHERE t.DateKey &lt;= TR.DateKey)
, ActiveJackpotCaptain  = (SELECT SUM(ActiveJackpotCaptain) FROM @TotalRegistrants t WHERE t.DateKey &lt;= TR.DateKey)
, TotalRegistrants = (SELECT SUM(TotalNewRegistrants) FROM @TotalRegistrants t WHERE t.DateKey &lt;= TR.DateKey)
, TotalActiveNewLogins = SUM(TotalActiveNewLogins)
, TotalActiveLogins = SUM(TotalActiveLogins)
FROM @TotalRegistrants TR
INNER JOIN @TotalLogins TL
ON TR.DateKey = TL.DateKey
INNER JOIN BIDW.dbo.DimDate D
ON TR.DateKey = D.DateKey
WHERE TR.DateKey = @StartDateKey 
group by TR.Datekey
, CalDayStr

UNION 

