        <CommandText>/* Default Dates */
/* Modified: 2014-07-09 */

-- as of the most recent date with data from the Player Registration table

SELECT StartDateValue = DATEADD(dd, CONVERT(INT, 0), (CONVERT(VARCHAR(10), MAX(EffectiveStartDate), 120)))
, StartDateValue29Days = DATEADD(dd, CONVERT(INT, -29), (CONVERT(VARCHAR(10), MAX(EffectiveStartDate), 120)))
FROM BIDW.dbo.DimPlayerRegistration</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
